	<Frame name="CooldownAlert" hidden="true" parent="UIParent" movable="true">
		<Size>
			<AbsDimension x="1200" y="32"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER" relativePoint="CENTER">
				<Offset>
					<AbsDimension x="0" y="0"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Layers>
			<Layer level="ARTWORK">
				<FontString name="$parentText" inherits="ZoneTextFont" text="&lt;Your Skill&gt; is ready!">
					<Size>
						<AbsDimension x="1200" y="0"/>
					</Size>
					<Anchor point="CENTER" relativePoint="CENTER">
						<Offset>
							<AbsDimension x="0" y="0"/>
						</Offset>
					</Anchor>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				this.state = 0;
				this.holdtime = 0;
				this.locked = true;
				CooldownAlert_OnLoad();
			</OnLoad>
			<OnUpdate>
				CooldownAlert_OnUpdate();
			</OnUpdate>
			<OnDragStart>
				if( not this.locked ) then
					this:StartMoving();
				end
			</OnDragStart>
			<OnDragStop>
				this:StopMovingOrSizing();
			</OnDragStop>
		</Scripts>
	</Frame>

